{
  "name": "andrejusk.github.io",
  "version": "3.2.0",
  "description": "andrejus.uk",
  "repository": "https://github.com/andrejusk/andrejusk.github.io",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw -b 'http://localhost:3000'",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "yarn start:hugo -- -D -F",
    "preview:webpack": "yarn start:webpack",
    "prebuild": "rimraf dist",
    "build": "yarn build:webpack && yarn build:hugo",
    "build:preview": "yarn build:webpack && yarn build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v --gc --minify --enableGitInfo",
    "build:hugo:preview": "yarn build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "author": "Andrejus Kostarevas",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.1",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^4.1.0",
    "highlightjs": "^9.12.0",
    "hugo-bin": "^0.44.3",
    "jquery": "^3.4.1",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "popper.js": "^1.15.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-font-magician": "^2.2.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.0.0",
    "typeface-inter": "^3.7.0",
    "typeface-raleway": "^0.0.75",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  }
}
